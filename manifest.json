{
  "name": "mdnteaser",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "adds text teaser to mdn",

  "permissions": [
    "tabs",
    "http://developer.mozilla.org/*",
    "https://developer.mozilla.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.8.0.min.js","background.js"]
    }
  ]
}
